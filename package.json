{
    "name": "vsc-css-support",
    "displayName": "vsc-css-support",
    "description": "Display the support of the css",
    "version": "0.0.1",
    "publisher": "avraammavridis",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
